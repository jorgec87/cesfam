<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:xe</property>
    <property name="hibernate.connection.username">cesfam_dba</property>
    <property name="hibernate.connection.password">cesfam</property>
    <mapping resource="cl/cesfam/entity/Stock.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Partida.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/FuncionarioFarmacia.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/FormularioMediamento.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/DetalleEntrega.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Medico.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/EventoStock.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Medicamento.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Componente.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Paciente.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/DetallePartida.hbm.xml"/>
    <mapping resource="cl/cesfam/entity/Composicion.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
